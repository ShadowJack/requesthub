<%= if @current_user do %>
  <%= button "Logout", to: session_path(@conn, :logout), method: :post %>
  <%= link "Profile", to: user_path(@conn, :edit, @current_user) %>
<% else %>
  <%= link "Sign up", to: user_path(@conn, :new) %>
  or
  <%= link "Login", to: session_path(@conn, :new) %>
<% end %>
